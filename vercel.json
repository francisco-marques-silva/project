{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/health",
      "dest": "api/index.js"
    },
    {
      "src": "/(css|js)/(.*)",
      "dest": "frontend/$1/$2"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "frontend/$1"
    },
    {
      "src": "/login",
      "dest": "frontend/login.html"
    },
    {
      "src": "/portal",
      "dest": "frontend/portal.html"
    },
    {
      "src": "/search-review",
      "dest": "frontend/search-review.html"
    },
    {
      "src": "/data-analysis",
      "dest": "frontend/data-analysis.html"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/index.html"
    }
  ]
}
